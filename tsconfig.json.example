{
  "compilerOptions": {
    "outDir": "dist/scripts",
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": false,
    "moduleResolution": "Node",
    "noImplicitAny": true,                /* Raise error on expressions and declarations with an implied 'any' type. */
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": [
    "src/scripts/**/*"
  ]
}